function fnCompile(){var e=$("#iTaxArea").val().split("\n");data={},data.code=[];for(var a=0;a<e.length;a++){var t={line:e[a]};data.code.push(t)}$.ajax({type:"POST",data:data,url:"/compiler",success:function(e){fnAlert("#errorAlert",e.error,errorInitialText),fnAlert("#warningAlert",e.warning,warningInitialText),fnAppendResultToTable(e)}})}function fnAlert(e,a,t){if(0!==a.length){$(e).html(t);for(var n=0;n<a.length;n++){var l=a[n];$(e).html($(e).html()+"<br/> Linha "+l.line+" : "+l.mess)}$(e).show()}else $(e).hide()}function fnAppendResultToTable(e){$("#tTable").show(),$("#tTableBody tr").remove();for(var a=e.data,t=0;t<a.length;t++)$("#tTableBody").append(fnBuildTableItem(a[t]))}function fnBuildTableItem(e){return"<tr><td>"+e.assembly+"</td><td>"+e.binary+"</td><td>"+e.hexa+"</td></tr>"}function fnCleanCode(){$("#iTaxArea").val("")}function fnSampleCode(){$.ajax({type:"GET",url:"/samplecode",success:function(e){$("#iTaxArea").val(e.val)}})}
